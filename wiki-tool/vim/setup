#steps
mddir .vim
cd .vim
git clone https://github.com/tpope/vim-pathogen
cd vim-pathogen
mv autoload ..
cd ..
sudo rm -fr vim-pathogen

>wiki-vim

mkdir bundle
cd bundle
git clone ...




#tags
cd ~/.vim/bundle
:Helptags
OK
all sub will has doc/tags


